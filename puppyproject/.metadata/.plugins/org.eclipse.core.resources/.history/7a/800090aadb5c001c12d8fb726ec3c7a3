	//펫목록 구현
	@Override
	public List<PetVO> petList() {
		List<PetVO> myList=null;
		myList=mypageDAO.petList();
		return myList;
	}

	//펫등록 구현
	@Override
	public int petInsert(PetVO pvo) {
		// TODO Auto-generated method stub
		return 0;
	}

}
